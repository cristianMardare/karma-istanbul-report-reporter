{
  "name": "karma-istanbul-report-reporter",
  "version": "0.0.4",
  "description": "A Karma plugin that allows the addition of custom reports to Istanbul code coverage tool",
  "main": "lib\\index.js",
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "test": "echo \"No tests yet\" & exit 0"
  },
  "keywords": [
    "karma-reporter",
    "karma-plugin",
    "istanbul",
    "istanbuljs"
  ],
  "author": "Cristian Mardare",
  "license": "ISC",
  "dependencies": {
    "istanbul-api": "1.2.1"
  },
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "semantic-release": "12.4.1",
    "validate-commit-message": "3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
